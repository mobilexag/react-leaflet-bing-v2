  "description": "Bing layer as React compoment for Leaflet",
  "main": "dist/react-leaflet-bing.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:lib": "rmdir /s ./lib",
    "compile:lib": "babel src --out-dir lib",
    "build:lib": "npm run compile:lib",
    "build:dist": "webpack --mode development src/index.js -o dist/react-leaflet-bing.js & webpack --mode production src/index.js -o dist/react-leaflet-bing.min.js",
    "build": "npm run build:lib && npm run build:dist",
    "example": "webpack-dev-server --config ./example/webpack.config.js",
    "lint": "eslint ./src",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:TA-Geoforce/react-leaflet-bing-v2.git"
  },
  "keywords": [
